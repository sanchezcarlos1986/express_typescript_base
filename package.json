{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "stubby": "stubby -d stubby/endpoints.yaml -s 3001 -w",
    "dev": "ts-node-dev src/index.ts",
    "mongodb": "brew services start mongodb-community",
    "build": "tsc",
    "start": "node dist",
    "pre-commit": "npm run branch-naming-check && lint-staged",
    "branch-naming-check": "branch-naming-check \"^(master|develop)|(feature|bugfix|refactor|ci|hotfix|jira|issue|renovate)/[a-zA-Z0-9._-]+$\"",
    "test": "NODE_ENV=test PORT=8080 jest --verbose",
    "test:watch": "npm run test -- --watch",
    "test:unit": "npm run test",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "mongoose": "^6.1.3",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.16.5",
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@innocells/branch-naming-check": "^1.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/morgan": "^1.9.3",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "eslint": "^8.5.0",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^27.4.5",
    "lint-staged": "^13.0.1",
    "prettier": "^2.5.1",
    "stubby": "^5.1.0",
    "supertest": "^6.1.6",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.3"
  },
  "lint-staged": {
    "./**/*.{js,ts}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
